                  rdfs:subClassOf HBP_CELL:0000033 .



HBP_CELL:0000030 rdf:type owl:Class ;

                  rdfs:label "Horizontal Pyramidal Cell"@en ;

                  nsu:definition "Horizontal tufted Pyramidal Cells have a horizontally extended dendrite with more branches than a typical basal dendrite."@en ;

