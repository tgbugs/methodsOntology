HBP_CELL:0000031 rdf:type owl:Class ;

                  rdfs:label "Bipolar Pyramidal Cell"@en ;

                  nsu:definition "Bipolar Pyramidal Cells have a typical apical dendrite towards pial with or without a small tuft and a big inverted (towards white matter) dendrite with more branches than other basal dendrites."@en ;

                  nsu:synonym "BPC"@en ;

                  rdfs:subClassOf HBP_CELL:0000018 .



HBP_CELL:0000032 rdf:type owl:Class ;

                  rdfs:label "Narrow Pyramidal Cell projecting to thalamus"@en ;

                  nsu:definition "Narrow Pyramidal Cells have a narrow-looking appearance with short basal and oblique dendrites and a small tuft."@en ;

                  nsu:synonym "NPC"@en ;

                  rdfs:subClassOf HBP_CELL:0000018 ;

                  rdfs:subClassOf HBP_CELL:0000034 .



